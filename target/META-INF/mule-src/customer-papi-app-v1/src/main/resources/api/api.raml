            example: !include /exchange_modules/6eecb176-c096-4b1c-b75f-6fab452ea227/common-fragment/1.0.6/examples/404-error-response-example.json      
      500:
        description: Internal Server Error
        body:
          application/json:
            type: error-dataType
            example: !include /exchange_modules/6eecb176-c096-4b1c-b75f-6fab452ea227/common-fragment/1.0.6/examples/500-error-response-example.json     

